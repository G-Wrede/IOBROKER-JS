// Um 18:00 Uhr am Vortag (verbleibende Tage = 1) erinnern, dass Abholung bevorsteht
schedule("0 18 * * *", async function () {
  if (getState("trashschedule.0.next.dateFound").val && getState("trashschedule.0.next.daysLeft").val == 1) {
    sendTo("telegram", "send", {
        text: (['Morgen wird die',getState("trashschedule.0.next.typesText").val,' abgeholt: '].join(''))
    });
  }
});
// Um 07:00 Uhr am Abholtag (verbleibende Tage = 0) erinnern, dass Abholung bevorsteht
schedule("0 7 * * *", async function () {
  if (getState("trashschedule.0.next.dateFound").val && getState("trashschedule.0.next.daysLeft").val == 0) {
    sendTo("telegram", "send", {
        text: (['Heute wird die',getState("trashschedule.0.next.typesText").val,' abgeholt: '].join(''))
    });
  }
});

