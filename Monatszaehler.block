schedule("1 0 1 * *", async function () {
  // Monatszaehler 0:00
  setState("0_userdata.0.Wasserzaehler.Monatszaehler"/*Monatszaehler*/, Math.round((parseFloat(getState("mqtt.0.Haus.Keller.Wasserzaehler.main.value").val) - getState("0_userdata.0.Wasserzaehler.Monatsvariable").val)*10000)/10000);
  setState("0_userdata.0.Wasserzaehler.Preis_Monat"/*Preis_Monat*/, Math.round((parseFloat(getState("0_userdata.0.Wasserzaehler.Preis_QM").val) * getState("0_userdata.0.Wasserzaehler.Monatszaehler").val)*100)/100);
  await wait(1000);
  // Preis Monat 0:01
  setState("0_userdata.0.Wasserzaehler.Preis_Monat"/*Preis_Monat*/, getState("0_userdata.0.Wasserzaehler.Preis_Monat").val, true);
  await wait(1000);
  // Monatsvariable 0:02
  setState("0_userdata.0.Wasserzaehler.Monatsvariable"/*Monatsvariable*/, getState("mqtt.0.Haus.Keller.Wasserzaehler.main.value").val);
});

