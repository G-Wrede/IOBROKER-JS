var ObjectID, SensorOnlineXY, CurrendName;

// Beschreibe diese Funktion â€¦
async function Objekt_Name_XY(ObjectID) {
    let name = getObject(ObjectID).common.name;
    return name;
}


schedule("2 8 * * *", async function () {
  var SensorOnlineXY_list = Array.prototype.slice.apply($("state[id=zigbee.*.*.battery]"));
  for (var SensorOnlineXY_index in SensorOnlineXY_list) {
    SensorOnlineXY = SensorOnlineXY_list[SensorOnlineXY_index];
    if (getState(SensorOnlineXY).val < 20) {
      CurrendName = await Objekt_Name_XY(SensorOnlineXY);
      sendTo("telegram", "send", {
          text: (['  Der Sensor','\n',[CurrendName,'\n',getState(SensorOnlineXY).val,'%'].join(''),'\n','hat einen schlechten Ladezustand','\n','Bitte Batterie austauschen.'].join(''))
      });
    }
  }
});

