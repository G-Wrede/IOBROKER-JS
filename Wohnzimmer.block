// Wohnzimmer http://192.168.10.3/
// Datenpunkt 0_userdata.0.Test_Ordner.Arbeitstag steuern
schedule("10 0 * * 1-5", async function () {
  if (getState("feiertage.0.heute.boolean").val == true) {
    setState("0_userdata.0.Haus.Rolladen.Arbeitstag"/*Arbeitstag*/, false);
  } else {
    setState("0_userdata.0.Haus.Rolladen.Arbeitstag"/*Arbeitstag*/, true);
  }
});
// An Arbeitstagen Rolladen per Astro hochfahren
schedule({astro: "dawn", shift: 0}, async function () {
  if (getState("0_userdata.0.Haus.Rolladen.Arbeitstag").val == true) {
    setState("shelly.0.SHSW-25#98CDAC247D35#1.Favorites.Position2"/*Set favorite position*/, true);
  }
});
// Rolladen per Astro runterfahren
schedule({astro: "sunsetStart", shift: NaN}, async function () {
  setState("shelly.0.SHSW-25#98CDAC247D35#1.Favorites.Position3"/*Set favorite position*/, true);
});
// um 9:01 prüfen ob kein Arbeitstag, dann Öffnen
schedule("1 9 * * *", async function () {
  if (getState("0_userdata.0.Haus.Rolladen.Arbeitstag").val == false) {
    setState("shelly.0.SHSW-25#98CDAC247D35#1.Favorites.Position2"/*Set favorite position*/, true);
  }
});

