// Kaminlicht in der Nacht an
schedule("*/10 * * * *", async function () {
  schedule({astro: "night", shift: 0}, async function () {
    setState("shelly.0.SHRGBW2#6EA5A2#1.lights.Switch"/*Switch*/, true);
  });
  schedule({astro: "dawn", shift: 0}, async function () {
    setState("shelly.0.SHRGBW2#6EA5A2#1.lights.Switch"/*Switch*/, false);
  });
});

