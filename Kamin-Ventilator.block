// Automatische Ventilator Steuerung vom Kamin
on({id: "zigbee.1.00158d0007e0aecd.temperature"/*Temperature*/, change: "any"}, async function (obj) {
  var value = obj.state.val;
  var oldValue = obj.oldState.val;
  if (getState("zigbee.1.00158d0007e0aecd.temperature").val > 25) {
    setState("zigbee.1.a4c1386491059cb9.state"/*On/off state of the switch*/, true);
  }
  if (getState("zigbee.1.00158d0007e0aecd.temperature").val < 25) {
    setState("zigbee.1.a4c1386491059cb9.state"/*On/off state of the switch*/, false);
  }
});

